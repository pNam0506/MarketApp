<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#606060"
    tools:context=".infoseller">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="410dp"
        android:layout_height="608dp"
        android:background="#ffffff"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.611" />

    <EditText
        android:id="@+id/phone"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/usernamewriteround"

        android:hint="เบอร์"
        android:padding="10dp"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Tphone" />

    <TextView
        android:id="@+id/Tname"
        android:layout_width="200dp"
        android:layout_height="22dp"
        android:text="ชื่อ - นามสกุล เจ้าของกิจการ"
        android:textColor="@color/black"
        android:textSize="18sp"


        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.046" />

    <EditText
        android:id="@+id/nameinfoSeller"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/usernamewriteround"
        android:hint="ชื่อ"
        android:padding="10dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.704"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Tname" />

    <TextView
        android:id="@+id/Tboot"
        android:layout_width="200dp"
        android:layout_height="22dp"
        android:text="ชื่อร้านค้า"
        android:textColor="@color/black"
        android:textSize="18sp"


        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.223" />

    <EditText
        android:id="@+id/boot"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/usernamewriteround"
        android:hint="ชื่อ"
        android:padding="10dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.606"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Tboot" />

    <TextView
        android:id="@+id/Temail"
        android:layout_width="200dp"
        android:layout_height="23dp"
        android:text="Email"
        android:textColor="@color/black"
        android:textSize="18sp"


        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.418" />

    <EditText
        android:id="@+id/email"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/usernamewriteround"
        android:hint="Email"
        android:padding="10dp"

        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.606"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Temail" />


    <TextView
        android:id="@+id/Tphone"
        android:layout_width="200dp"
        android:layout_height="23dp"
        android:text="เบอร์โทรศัพท์"
        android:textColor="@color/black"
        android:textSize="18sp"


        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.454"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.606" />

    <TextView
        android:id="@+id/Ttype"
        android:layout_width="200dp"
        android:layout_height="22dp"
        android:text="สินค้าที่ขาย"
        android:textColor="@color/black"
        android:textSize="18sp"


        app:layout_constraintBottom_toBottomOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.457"
        app:layout_constraintStart_toStartOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.805" />

    <EditText
        android:id="@+id/product"
        android:layout_width="350dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/usernamewriteround"
        android:hint="สินค้าที่ขาย"
        android:padding="10dp"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.606"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Ttype" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="7dp"
        android:background="@drawable/shop_svgrepo_com_1"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="204dp"
        android:layout_height="57dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="16dp"
        android:text="ลงทะเบียน"
        android:textColor="#ffffff"
        android:textSize="45sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="164dp"
        android:layout_height="39dp"
        android:layout_marginTop="12dp"
        android:text="เจ้าของธุรกิจ"
        android:textColor="#ffffff"
        android:textSize="30sp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.58"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toBottomOf="@+id/textView5"
        app:layout_constraintVertical_bias="0.04" />

    <Button
        android:id="@+id/save"
        android:layout_width="108sp"
        android:layout_height="50sp"
        android:layout_marginTop="20dp"
        android:background="@drawable/bgsave"
        android:onClick="Save"
        android:text="บันทึก"
        android:textColor="#ffffff"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.15" />

    <TextView
        android:layout_width="40sp"
        android:layout_height="40sp"
        android:background="@drawable/account_avatar_profile_user_6_svgrepo_com"
        app:layout_constraintBottom_toBottomOf="@+id/Tname"
        app:layout_constraintEnd_toStartOf="@+id/Tname"
        app:layout_constraintHorizontal_bias="0.785"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="40sp"
        android:layout_height="40sp"
        android:layout_marginStart="44dp"

        android:layout_marginEnd="12dp"
        android:background="@drawable/shop_svgrepo_com_1"
        app:layout_constraintBottom_toBottomOf="@+id/Tboot"
        app:layout_constraintEnd_toStartOf="@+id/Tboot"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="39dp"
        android:layout_height="42dp"
        android:layout_marginEnd="12dp"


        android:background="@drawable/email_svgrepo_com"
        app:layout_constraintBottom_toBottomOf="@+id/Temail"
        app:layout_constraintEnd_toStartOf="@+id/Temail"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="39dp"
        android:layout_height="42dp"
        android:layout_marginEnd="12dp"
        android:background="@drawable/phone_call_telephone_svgrepo_com"
        app:layout_constraintBottom_toBottomOf="@+id/Tphone"
        app:layout_constraintEnd_toStartOf="@+id/Tphone"
        app:layout_constraintHorizontal_bias="0.977"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="40sp"
        android:layout_height="40sp"
        android:layout_marginEnd="8dp"

        android:background="@drawable/food_stall_stall_svgrepo_com"
        app:layout_constraintBottom_toBottomOf="@+id/Ttype"
        app:layout_constraintEnd_toStartOf="@+id/Ttype"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>