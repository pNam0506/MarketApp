       >
    <GridLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:rowCount="1"
        android:columnCount="2"
        >
     <LinearLayout
         android:layout_height="50dp"
         android:layout_width="75dp"
         android:layout_margin="10dp"
