          android:gravity="center"
          >



      </LinearLayout>
      <LinearLayout
          android:layout_height="20dp"
          android:layout_width="30dp"
          android:layout_marginTop="5dp"
          android:layout_marginLeft="5dp"
          android:background="#FF5CE4"
          android:gravity="center"
